			,USER_EMAIL 
			,USER_MEMBER 
			,USER_STATUS
			,USER_WARNING
			,USER_IMG
			,USER_NICK 
		) VALUES (
			 #{user_id}
			,#{user_pw}
			,#{user_name}
			,#{user_age}
			,#{user_addr}
			,#{user_addrdetail}
			,#{user_phone}
			,#{user_email}
			,#{user_member}
			,'N
			,0
			,'test'
			,#{user_nick}
			)
	</insert>
	
